{
  "id": "default",
  "description": "Default settings",
  "engineFactory": "org.template.ecommercerecommendation.ECommerceRecommendationEngine",
  "datasource": {
    "params" : {
      "appName": "wotm"
    }
  },
  "algorithms": [
    {
      "name": "ecomm",
      "params": {
        "appName": "wotm",
        "unseenOnly": true,
        "seenEvents": ["like", "dislike"],
        "similarEvents": ["like"],
        "rank": 3,
        "numIterations" : 20,
        "lambda": 0.09,
        "seed": 3
      }
    }, 
    {
      "name": "dislike_ecomm",
      "params": {
        "appName": "wotm",
        "unseenOnly": true,
        "seenEvents": ["like", "dislike"],
        "similarEvents": ["dislike"],
        "rank": 8,
        "numIterations" : 20,
        "lambda": 0.05,
        "seed": 3
      }
    }
  ]
}
